<!DOCTYPE html>
<html lang="en">
    <%- include('./Partials/head', {title: 'Form Report'}) %>
<h1>list Novels</h1>
<body>
    <table border="1">
        <thead>
            <th> Title</th>
            <th> Pages</th>
            <th> authorCode</th>
            <th> authorName</th>
            <th> action</th>
        </thead>
    <tbody>
        <% data.forEach(e=> {%>
        <tr>
     <td><%= e.title %></td> 
     <td><%= e.pages %></td> 
     <td><%= e.authorCode %></td> 
     <td><%= e.Author.name %></td> 
     <a href="/novels/detail"><td><button type="submit">Read Detail</button></td></a>
    <%}) %> 
</tr>   
</table>
    </tbody>
</body>
</html>